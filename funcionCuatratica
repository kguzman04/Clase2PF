from math import sqrt

def formula(a,b,c):
    x1 = (-b + (sqrt((b*b)-(4*a*c))))/(2*a) #Con positivo
    x2 = (-b - (sqrt((b*b)-(4*a*c))))/(2*a) #Con negativo
    print("a es igual a ",a)
    print("b es igual a ",b)
    print("c es igual a ",c)
    print("X1 es igual a ",x1)
    print("X2 es igual a ",x2)


a = float(input(("Dame el valor de a")))
b = float(input(("Dame el valor de b")))
c = float(input(("Dame el valor de c"))) 
   


formula(a,b,c)

